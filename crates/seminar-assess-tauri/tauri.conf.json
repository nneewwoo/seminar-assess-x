{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Seminar Assess",
  "identifier": "com.seminar-assess.app",
  "build": {
    "beforeDevCommand": "pnpm dev:android",
    "beforeBuildCommand": "pnpm build:android -- --mode development",
    "frontendDist": "../../apps/android/build",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": false,
    "category": "Survey",
    "targets": "all",
    "icon": [
      "icons/dev/32x32.png",
      "icons/dev/128x128.png",
      "icons/dev/128x128@2x.png",
      "icons/dev/icon.icns",
      "icons/dev/icon.ico"
    ]
  },
  "app": {
    "windows": [
      {
        "title": "Seminar Assess",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  }
}
